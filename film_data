FILM_ID

SELECT MIN(film_id) AS min_film_id, MAX(film_id) AS max_film_id, AVG(film_id) AS avg_film_id
FROM film



min_film_id
1




RELEASE_YEAR

SELECT MIN(release_year) AS min_release_year, MAX(release_year) AS max_release_year, AVG(release_year) AS avg_release_year
FROM film

min_release_year
2006



LANGUAGE_ID
SELECT MIN(language_id) AS min_language_id, MAX(language_id) AS max_language_id, AVG(language_id) AS avg_language_id
FROM film


min_language_id
1





RENTAL_DURATION
SELECT MIN(rental_duration) AS min_rental_duration, MAX(rental_duration) AS max_rental_duration, AVG(rental_duration) AS avg_rental_duration
FROM film

min_rental_duration
3









RENTAL_RATE

SELECT MIN(rental_rate) AS min_rental_rate, MAX(rental_rate) AS max_rental_rate, AVG(rental_rate) AS avg_rental_rate
FROM film

min_rental_rate
0.99


LENGTH
SELECT MIN(length) AS min_length, MAX(length) AS max_length, AVG(length) AS avg_length
FROM film

min_length
46













REPLACEMENT_COST
SELECT MIN(replacement_cost) AS min_replacement_cost, MAX(replacement_cost) AS max_replacement_cost, AVG(replacement_cost) AS avg_replacement_cost
FROM film

